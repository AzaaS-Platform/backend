{
    "name": "azaas-backend",
    "version": "1.0.0",
    "description": "AzaaS Serverless Backend",
    "scripts": {
        "test": "jest --no-cache",
        "local": "sls offline -s dev",
        "build": "sls package",
        "deploy": "sls deploy --verbose",
        "deploy-prod": "sls deploy --verbose -s prod",
        "invoke": "sls invoke local -f",
        "lint-check": "eslint . --ext src/.js,.ts",
        "lint": "eslint . --ext src/.js,.ts --fix"
    },
    "dependencies": {
        "jsonwebtoken": "^8.5.1",
        "bcrypt": "^5.0.0",
        "moment": "^2.29.0",
        "source-map-support": "^0.5.19",
        "uuid": "^7.0.3"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.62",
        "@types/bcrypt": "^3.0.0",
        "@types/jest": "^25.2.3",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/moment": "^2.13.0",
        "@types/node": "^10.17.35",
        "@types/uuid": "^7.0.4",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "fork-ts-checker-webpack-plugin": "^3.0.1",
        "jest": "^25.5.4",
        "prettier": "^1.19.1",
